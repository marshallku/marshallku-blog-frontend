{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": ["dist/**", "storybook-static/**"],
            "dependsOn": ["^build"]
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true,
            "env": ["NODE_ENV", "NEXT_PUBLIC_BLOG_ORIGIN", "NEXT_PUBLIC_CDN_URL"]
        },
        "clean": {
            "cache": false
        }
    }
}
