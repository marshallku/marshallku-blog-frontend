        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "devDependencies": {
        "@marshallku/eslint-config": "workspace:*",
        "@marshallku/typescript-config": "^0.0.4",
        "eslint": "^9.17.0",
        "typescript-eslint": "^8.18.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
