{
    "name": "@marshallku/utils",
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts",
        "dev": "tsup src/index.ts --format esm --watch --dts",
        "lint": "eslint \"src/**/*.ts*\"",
        "test": "vitest run",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "devDependencies": {
        "@marshallku/eslint-config": "workspace:*",
        "@marshallku/typescript-config": "^0.0.4",
        "eslint": "^9.17.0",
        "typescript-eslint": "^8.18.2",
        "react": "^19.2.0",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4",
        "vitest-monocart-coverage": "^3.0.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
